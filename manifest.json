{
    "manifest_version": 3,
    "name": "Acronymus",
    "version": "1.0.0",
    "description": "Replaces abbreviations and acronyms to their full meaning",
    "icons": {"96": "brain64.png"},
    "action": {"default_icon": "brain64.png"},
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end",
            "all_frames": true
        }
    ]
}